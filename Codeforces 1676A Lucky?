#include <bits/stdc++.h>
using namespace std;
int main ()
{
    int t;
    cin>>t;
    while(t--){
    string s;
    cin>>s;
    int sum1=0,sum2=0;
    for(int i=0;i<s.size()/2;i++)
    {
        int dig = s[i]-'0';
        sum1=sum1+dig;
    }
    for(int i=s.size()/2;i<s.size();i++){
        int dig2=s[i]-'0';
        sum2=sum2+dig2;
    }
    if(sum1==sum2){
        cout<<"YES\n";
    }
    else{
        cout<<"NO\n";
    }
    }
}
